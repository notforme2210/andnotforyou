#!/bin/bash
az group create --name myResourceGroup --location eastus
wget https://raw.githubusercontent.com/notforme2210/andnotforyou/master/azuuu.sh
chmod +x azuuu.sh
az vm create --resource-group myResourceGroup --name myVM23 --size Standard_DS3_v2 --location eastus --image Canonical:UbuntuServer:18.04-LTS:latest --admin-username azureuser  --generate-ssh-keys  --custom-data azuuu.sh
az vm create --resource-group myResourceGroup --name myVM1 --size Standard_DS3_v2 --location westeurope --image Canonical:UbuntuServer:18.04-LTS:latest --admin-username azureuser --generate-ssh-keys --custom-data azuuu.sh &
az vm create --resource-group myResourceGroup --name myVM2 --size Standard_DS3_v2 --location centralus --image Canonical:UbuntuServer:18.04-LTS:latest --admin-username azureuser --generate-ssh-keys --custom-data azuuu.sh &
