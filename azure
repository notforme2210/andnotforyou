az vm create --resource-group myResourceGroup --name myVM9 --size Standard_DS3_v2 --location canadacentral --image Canonical:UbuntuServer:18.04-LTS:latest --admin-username azureuser --generate-ssh-keys --custom-data azuuu.sh &
az vm create --resource-group myResourceGroup --name myVM10 --size Standard_DS3_v2 --location westindia --image Canonical:UbuntuServer:18.04-LTS:latest --admin-username azureuser --generate-ssh-keys --custom-data azuuu.sh	
